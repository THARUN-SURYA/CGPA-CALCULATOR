marks = int(input("enter the marks:"))
if(marks >= 90):
grade = "O"
elif(marks >=80 and <90):
grade = "A+"
elif(marks >=70 and <80);
grade = "A"
elfi(marks >=70 and <60):
grade = "B+"
elif(marks >=60 and <50):
grade = "B"
elif(marks >=50 and <40):
grade = "c+"
else:
grade = "U"
printf("The grade is _ ",grade)
